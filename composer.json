{
    "name": "jameslkingsley/laravel-mentions",
    "description": "End-to-end mentions in Laravel 5.",
    "keywords": [
        "laravel",
        "mentions"
    ],
    "homepage": "https://github.com/jameslkingsley/laravel-mentions",
    "license": "MIT",
    "authors": [
        {
            "name": "James Kingsley",
            "email": "jameslkingsley@outlook.com",
            "homepage": "https://github.com/jameslkingsley",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "illuminate/support": "~5.5.0",
        "illuminate/database": "~5.5.0",
        "illuminate/http": "~5.5.0",
        "illuminate/routing": "~5.5.0"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.7",
        "orchestra/testbench": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "Kingsley\\Mentions\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kingsley\\Mentions\\Test\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor\\bin\\phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kingsley\\Mentions\\MentionServiceProvider"
            ]
        }
    }
}
